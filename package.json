{
  "name": "@rbxts/reflection",
  "version": "1.0.2",
  "description": "Library that allows you to work with types in runtime!",
  "main": "out/init.lua",
  "scripts": {
    "build": "rbxtsc",
    "watch": "rbxtsc -w",
    "test:watch": "rbxtsc -w --type=game -p test -i test/include",
    "serve": "rojo serve test/default.project.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "@rbxts",
    "transformer",
    "reflection",
    "reflect",
    "type",
    "types",
    "reflection-runtime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/roblox-ts-reflection/reflection"
  },
  "author": "Tesmi (https://github.com/Tesmi-Develop)",
  "license": "MIT",
  "types": "out/index.d.ts",
  "files": [
    "out",
    "!**/*.tsbuildinfo",
    "!**/*.spec.lua"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^3.0.0-types.0",
    "@rbxts/types": "^1.0.813",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-roblox-ts": "^0.0.36",
    "prettier": "^3.2.4",
    "rbxts-transformer-reflection": "latest",
    "roblox-ts": "^3.0.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@rbxts/reflection-runtime": "latest"
  }
}